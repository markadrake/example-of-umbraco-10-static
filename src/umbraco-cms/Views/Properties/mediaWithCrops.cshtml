@using Umbraco.Cms.Core.Models
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<MediaWithCrops>
@{
    if (Model == null || Model.Content == null)
        return;
}
                "image": {
                    "id": @Model.Id,
                    "key": "@Model.Key",
                    "url": "@Model.Content.Url()",
                    "crops": [
                    @foreach (var crop in Model.LocalCrops.Crops)
                    {
                        @:{
                            @:"alias": "@crop.Alias",
                            @:"width": @crop.Width,
                            @:"height": @crop.Height
                        @:},
                    }
                    ]
                }